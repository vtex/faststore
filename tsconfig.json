{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowJs": false,
    "declaration": true,
    "strict": true,
    "moduleResolution": "node",
    "typeRoots": ["./typings"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
