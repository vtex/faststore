{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["OCLIF_COMPILATION", "NODE_ENV", "DISABLE_3P_SCRIPTS"],
  "pipeline": {
    "site#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "start": {},
    "size": {},
    "dev": {
      "cache": false
    }
  }
}
