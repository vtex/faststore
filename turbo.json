{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "OCLIF_COMPILATION",
    "NODE_ENV",
    "DISABLE_3P_SCRIPTS"
  ],
  "pipeline": {
    "site#build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "BASE_SITE_URL"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "start": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "size": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "dev:server": {
      "cache": false
    },
    "@faststore/api#dev:graphql": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "@faststore/api#build:cjs": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "@faststore/api#build:esm": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
