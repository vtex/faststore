{
  "name": "@vtex/gatsby-plugin-nginx",
  "version": "0.328.2-alpha.5",
  "description": "Gatsby plugin to generate a nginx configuration template.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/gatsby-plugin-nginx.esm.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src",
    "gatsby-*.js"
  ],
  "scripts": {
    "develop": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "vtex",
    "nginx",
    "gatsby-plugin-nginx"
  ],
  "size-limit": [
    {
      "path": "dist/gatsby-plugin-nginx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gatsby-plugin-nginx.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "gatsby": "^3.5.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/webpack-assets-manifest": "^5.0.0",
    "gatsby": "^3.5.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "kebab-hash": "^0.1.2",
    "webpack-assets-manifest": "^5.0.6"
  }
}
