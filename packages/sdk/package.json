    "test": "jest",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/react": "^14.3.0",
    "@types/jest": "29.1.0",
    "@types/node": "^18.11.16",
    "fake-indexeddb": "^3.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "size-limit": "^7.0.8",
    "ts-jest": "29.1.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@types/react": "^18.2.42",
    "idb-keyval": "^5.1.3"
  }
}
