{
  "name": "@vtex/faststore-sdk",
  "version": "3.81.0",
  "description": "Hooks for creating your next component library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtex/faststore",
    "directory": "packages/sdk"
  },
  "browserslist": "supports es6-module and not dead and last 2 version",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./*": [ "./dist/*.d.ts", "./dist/*.ts", "./dist/*.js"]
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "jest",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "engines": {
    "node": "22.19.0"
  },
  "peerDependencies": {
    "react": "catalog:"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/react": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "fake-indexeddb": "^3.1.3",
    "jest": "catalog:",
    "jest-environment-jsdom": "^29.7.0",
    "react": "catalog:",
    "size-limit": "^7.0.8",
    "ts-jest": "catalog:",
    "tslib": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  },
  "dependencies": {
    "fast-deep-equal": "catalog:",
    "idb-keyval": "^5.1.3",
    "zustand": "^5.0.5"
  }
}
