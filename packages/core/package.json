{
  "name": "@faststore/core",
  "version": "3.82.0",
  "license": "MIT",
  "repository": "vtex/faststore",
  "browserslist": "supports es6-module and not dead",
  "main": "./index.ts",
  "files": [
    "src",
    "@generated",
    "api",
    "cms",
    "discovery.config.js",
    "discovery.config.default.js",
    "discovery.config.user.js",
    "next-seo.config.ts"
  ],
  "exports": {
    ".": "./index.ts",
    "./ui": "./src/components/ui/index.ts",
    "./pages/*": "./src/pages/*.tsx",
    "./handlers/*": "./src/pages/api/*.ts",
    "./generated/*": "./@generated/*.ts",
    "./with-faststore": "./src/utils/withFaststore.js",
    "./config": "./discovery.config.js",
    "./typings/*": "./src/typings/*.d.ts",
    "./src/tsx/*": "./src/*.tsx",
    "./src/ts/*": "./src/*.ts",
    "./api": "./api/index.ts",
    "./experimental": "./src/experimental/index.ts",
    "./experimental/server": "./src/experimental/server/index.ts"
  },
  "bin": {
    "faststore-core": "./bin/generate-types.js"
  },
  "scripts": {
    "format:generated": "prettier --write \"@generated/**/*.{ts,js,tsx,jsx,json}\" --loglevel error",
    "generate": "node ./bin/generate-types.js true && na run format:generated",
    "prepack": "na run partytown && na run generate",
    "prebuild": "na run partytown && na run generate",
    "predev": "na run partytown && na run generate",
    "build": "echo '[@faststore/core] - bundling must be done on starter store side'",
    "dev": "echo '[@faststore/core] - DevServer is throug starter store'",
    "dev-only": "na run dev",
    "clean": "rm -r .next",
    "serve": "echo '[@faststore/core] - Starter devServer is throug starter store'",
    "test": "jest",
    "test:e2e": "playwright test",
    "lhci": "lhci autorun",
    "postinstall": "node postinstall.js && is-ci || echo Skipped postinstall step for @faststore/core",
    "partytown": "partytown copylib ./public/~partytown"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "node": "^20",
    "pnpm": "9.15.1",
    "yarn": "1.22.22"
  },
  "sideEffects": false,
  "dependencies": {
    "@antfu/ni": "catalog:",
    "@builder.io/partytown": "^0.6.1",
    "@envelop/core": "^5.0.2",
    "@envelop/graphql-jit": "^8.0.3",
    "@envelop/parser-cache": "^6.0.2",
    "@envelop/validation-cache": "^6.0.2",
    "@faststore/api": "workspace:*",
    "@faststore/graphql-utils": "workspace:*",
    "@faststore/lighthouse": "workspace:*",
    "@faststore/sdk": "workspace:*",
    "@faststore/ui": "workspace:*",
    "@graphql-codegen/cli": "catalog:",
    "@graphql-codegen/client-preset": "4.2.6",
    "@graphql-codegen/typescript": "catalog:",
    "@graphql-codegen/typescript-operations": "4.2.1",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "catalog:",
    "@graphql-tools/schema": "catalog:",
    "@graphql-tools/utils": "catalog:",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.0",
    "@vtex/client-cms": "^0.2.16",
    "@vtex/client-cp": "0.3.0",
    "@vtex/prettier-config": "1.0.0",
    "autoprefixer": "^10.4.0",
    "cookie": "catalog:",
    "css-loader": "^6.7.1",
    "deepmerge": "^4.3.1",
    "draftjs-to-html": "^0.9.1",
    "fast-deep-equal": "catalog:",
    "fs-extra": "^10.1.0",
    "include-media": "^1.4.10",
    "isomorphic-unfetch": "^3.1.0",
    "next-seo": "^6.6.0",
    "postcss": "^8.4.4",
    "prettier": "^2.2.0",
    "react-intersection-observer": "^8.32.5",
    "sass": "^1.44.0",
    "sass-loader": "^12.6.0",
    "sharp": "^0.32.6",
    "style-loader": "^3.3.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@envelop/testing": "^6.0.0",
    "@jest/globals": "catalog:",
    "@lhci/cli": "^0.9.0",
    "@playwright/test": "^1.55.0",
    "@types/cookie": "^0.6.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "catalog:",
    "@types/react": "catalog:",
    "@vtex/prettier-config": "1.0.0",
    "axe-core": "^4.8.1",
    "dotenv": "^16.4.5",
    "is-ci": "^3.0.0",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "next": "catalog:",
    "graphql": "catalog:"
  }
}
