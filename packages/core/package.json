{
  "name": "nextjs.store",
  "private": true,
  "version": "0.221.0",
  "license": "MIT",
  "browserslist": "supports es6-module and not dead",
  "scripts": {
    "generate": "graphql-codegen",
    "build": "yarn partytown && next build",
    "develop": "yarn partytown && next dev",
    "clean": "rm -r .next",
    "serve": "next start",
    "test": "cypress open",
    "lhci": "lhci autorun",
    "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json}\"",
    "lint": "eslint --ext js,ts,jsx,tsx .",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "postinstall": "is-ci || husky install",
    "partytown": "partytown copylib ./public/~partytown",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.5.4",
    "@envelop/core": "^1.2.0",
    "@envelop/graphql-jit": "^1.1.1",
    "@envelop/parser-cache": "^2.2.0",
    "@envelop/validation-cache": "^2.2.0",
    "@faststore/api": "^1.8.36",
    "@faststore/sdk": "^1.8.36",
    "@faststore/ui": "^1.8.36",
    "@vtex/graphql-utils": "^1.8.36",
    "graphql": "^15.0.0",
    "include-media": "^1.4.10",
    "next": "^12.1.6",
    "next-seo": "^5.4.0",
    "nextjs-progressbar": "^0.0.14",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-intersection-observer": "^8.32.5",
    "sass": "^1.44.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.10",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@lhci/cli": "^0.9.0",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@testing-library/cypress": "^8.0.0",
    "@types/cypress": "^1.1.3",
    "@vtex/lighthouse-config": "^1.8.36",
    "@vtex/prettier-config": "1.0.0",
    "@vtex/tsconfig": "0.6.0",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.3.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-remove-graphql-queries": "^4.11.1",
    "css-loader": "^6.7.1",
    "cypress": "6.6.0",
    "cypress-axe": "^0.13.0",
    "cypress-wait-until": "^1.7.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-vtex-react": "^9.0.0",
    "husky": "^5.2.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.4",
    "prettier": "^2.2.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2",
    "webpack": "^5.72.0"
  },
  "resolutions": {
    "@types/react": "^17.x"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  }
}
