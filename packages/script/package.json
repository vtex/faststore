{
  "name": "@vtex/faststore-script",
  "version": "0.0.0",
  "description": "FastStore Script Module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "vtex/faststore",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "22.19.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@next/third-parties": "catalog:",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "catalog:"
  },
  "peerDependencies": {
    "next": ">=15",
    "@next/third-parties": ">=15",
    "react": ">=19",
    "react-dom": ">=19"
  }
}
