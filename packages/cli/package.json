{
  "name": "@vtex/faststore-cli",
  "version": "3.89.2",
  "description": "FastStore CLI",
  "author": "Emerson Laurentino @emersonlaurentino",
  "bin": {
    "faststore": "./bin/run"
  },
  "homepage": "https://github.com/vtex/faststore",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "vtex/faststore",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@antfu/ni": "catalog:",
    "@inquirer/prompts": "^5.1.2",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3",
    "@vtex/faststore-core": "workspace:*",
    "chalk": "~4.1.2",
    "chokidar": "^3.5.3",
    "degit": "^2.8.4",
    "dotenv": "^16.4.5",
    "fs-extra": "^10.1.0",
    "ora": "5.4.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@inquirer/confirm": "^5.1.19",
    "@types/chai": "^4",
    "@types/degit": "^2.8.6",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "chai": "^4",
    "globby": "catalog:",
    "graphql": "catalog:",
    "jest": "catalog:",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-jest": "catalog:",
    "ts-node": "^10.9.1",
    "tslib": "catalog:"
  },
  "oclif": {
    "bin": "faststore",
    "dirname": "faststore",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "test": "jest src/**/*.test.ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "na run && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "bugs": "https://github.com/vtex/faststore/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
