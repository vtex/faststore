{
  "name": "@faststore/cli",
  "version": "2.0.30-alpha.0",
  "description": "FastStore CLI",
  "author": "Emerson Laurentino @emersonlaurentino",
  "bin": {
    "faststore": "./bin/run"
  },
  "homepage": "https://github.com/vtex/faststore",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "vtex/faststore",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.16.4",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.3",
    "chalk": "~4.1.2",
    "chokidar": "^3.5.3",
    "deepmerge": "^4.2.2",
    "fs-extra": "^10.1.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@faststore/shared": "^2.0.30-alpha.0",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.63",
    "chai": "^4",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.19.1"
  },
  "oclif": {
    "bin": "faststore",
    "dirname": "faststore",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint src/**/*.ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/vtex/faststore/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
