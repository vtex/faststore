{
  "name": "@faststore/cli",
  "version": "3.97.0-dev.3",
  "description": "FastStore CLI",
  "author": "Emerson Laurentino @emersonlaurentino",
  "type": "module",
  "bin": {
    "faststore": "./bin/run.js"
  },
  "homepage": "https://github.com/vtex/faststore",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./runner": "./bin/run.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vtex/faststore",
    "directory": "packages/cli"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@faststore/api": "workspace:*",
    "@faststore/core": "workspace:*",
    "@graphql-tools/merge": "catalog:",
    "@graphql-tools/utils": "catalog:",
    "@inquirer/confirm": "^5.1.19",
    "@inquirer/prompts": "^5.1.2",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3",
    "chalk": "~4.1.2",
    "chokidar": "^3.5.3",
    "degit": "^2.8.4",
    "dotenv": "^16.4.5",
    "fs-extra": "^11",
    "globby": "^15.0.0",
    "ora": "5.4.1",
    "@vtex/faststore-sdk": "0.1.1-canary.3"
  },
  "devDependencies": {
    "@antfu/ni": "catalog:",
    "graphql": "catalog:",
    "@graphql-codegen/cli": "catalog:",
    "@parcel/watcher": "^2.5.1",
    "@types/degit": "^2.8.6",
    "@types/fs-extra": "^11",
    "@types/node": "catalog:",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "faststore",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "commands"
    },
    "dirname": "faststore",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "scripts": {
    "build": "tsup-node",
    "test": "vitest run src/**/*.test.ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "na run build && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "bugs": "https://github.com/vtex/faststore/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@antfu/ni": "catalog:",
    "@parcel/watcher": "^2.5.1",
    "graphql": "catalog:"
  },
  "engines": {
    "node": ">=20"
  }
}
