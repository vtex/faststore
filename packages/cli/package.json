{
  "name": "@faststore/cli",
  "version": "3.0.149",
  "description": "FastStore CLI",
  "author": "Emerson Laurentino @emersonlaurentino",
  "bin": {
    "faststore": "./bin/run"
  },
  "homepage": "https://github.com/vtex/faststore",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "vtex/faststore",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@faststore/core": "^3.0.149",
    "@inquirer/prompts": "^5.1.2",
    "@oclif/core": "^1.16.4",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.3",
    "chalk": "~4.1.2",
    "chokidar": "^3.5.3",
    "degit": "^2.8.4",
    "dotenv": "^16.4.5",
    "fs-extra": "^10.1.0",
    "ora": "5.4.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@faststore/eslint-config": "^3.0.135",
    "@faststore/shared": "^3.0.135",
    "@types/chai": "^4",
    "@types/degit": "^2.8.6",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.63",
    "chai": "^4",
    "eslint": "7.32.0",
    "jest": "^29.7.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "oclif": {
    "bin": "faststore",
    "dirname": "faststore",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint src/**/*.ts",
    "test": "jest src/**/*.test.ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "na run lint",
    "prepack": "na run && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "bugs": "https://github.com/vtex/faststore/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
