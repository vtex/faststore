{
  "name": "@vtex/faststore",
  "version": "4.0.0",
  "description": "VTEX FastStore",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "vtex/faststore",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "22.19.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "catalog:"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "next": ">=15"
  }
}
