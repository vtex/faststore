{
  "name": "@faststore/ui",
  "version": "3.97.0-dev.2",
  "description": "A lightweight, framework agnostic component library for React",
  "author": "emersonlaurentino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtex/faststore",
    "directory": "packages/ui"
  },
  "browserslist": "supports es6-module and not dead and last 2 version",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./src/*": "./src/*"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/cjs/index.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/es/index.mjs",
      "limit": "40 KB"
    }
  ],
  "dependencies": {
    "@faststore/components": "workspace:*",
    "include-media": "^1.4.10",
    "modern-normalize": "^1.1.0",
    "react-swipeable": "catalog:",
    "tabbable": "catalog:"
  },
  "peerDependencies": {
    "@faststore/components": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/tabbable": "catalog:",
    "babel-loader": "^8.2.5",
    "size-limit": "^7.0.8",
    "tslib": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
