{
  "name": "@faststore/api",
  "version": "3.80.0",
  "license": "MIT",
  "repository": "vtex/faststore",
  "typings": "dist/src/index.d.ts",
  "module": "dist/es/index.mjs",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./*": "./dist/*",
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "browserslist": "node 20",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "graphql-codegen && vite build --watch",
    "dev:server": "echo '\n\n -> need to fix this ##tsx --no-cache ./local/server.ts'",
    "build": "graphql-codegen && vite build",
    "test:int": "jest --projects jest.int.config.js",
    "test:intc": "jest --projects jest.int.config.js --coverage",
    "test:unit": "jest --projects jest.unit.config.js --silent",
    "test:unitc": "jest --projects jest.unit.config.js --silent --coverage",
    "test": "pnpm run test:unit && pnpm run test:int",
    "generate": "graphql-codegen && vite build"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.3",
    "@graphql-tools/utils": "catalog:",
    "@rollup/plugin-graphql": "^1.0.0",
    "@types/crypto-js": "^4.2.2",
    "cookie": "catalog:",
    "crypto-js": "^4.2.0",
    "dataloader": "^2.1.0",
    "fast-deep-equal": "catalog:",
    "p-limit": "^3.1.0",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "catalog:",
    "@graphql-codegen/typescript": "catalog:",
    "@graphql-tools/merge": "catalog:",
    "@parcel/watcher": "^2.4.0",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.16",
    "@types/jest": "catalog:",
    "@types/sanitize-html": "^2.9.1",
    "concurrently": "^6.2.1",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "graphql": "catalog:",
    "graphql-tag": "^2.12.6",
    "jest": "catalog:",
    "nodemon": "^3.0.2",
    "ts-jest": "catalog:",
    "tslib": "catalog:",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "graphql": "catalog:"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
