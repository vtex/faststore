{
  "name": "@vtex/faststore-api",
  "version": "3.89.2",
  "repository": "vtex/faststore",
  "typings": "./dist/src/index.d.ts",
  "module": "dist/es/index.mjs",
  "main": "dist/cjs/index.js",
  "types": "./dist/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./*": "./dist/*",
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "browserslist": "node 20",
  "engines": {
    "node": "22.19.0"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test:int": "vitest run ./test/integration",
    "test:intc": "pnpm run test:int --coverage",
    "test:unit": "vitest run ./test/unit",
    "test:unitc": "pnpm run test:unit --coverage",
    "test": "pnpm run test:unit && pnpm run test:int",
    "generate": "graphql-codegen",
    "dev:server": "pnpm build && node ./local/server.mjs"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.3",
    "@graphql-tools/utils": "catalog:",
    "@rollup/plugin-graphql": "^1.0.0",
    "@types/crypto-js": "^4.2.2",
    "cookie": "catalog:",
    "crypto-js": "^4.2.0",
    "dataloader": "^2.1.0",
    "fast-deep-equal": "catalog:",
    "p-limit": "^3.1.0",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@envelop/core": "^5.3.1",
    "@envelop/graphql-jit": "^8.0.4",
    "@envelop/parser-cache": "^6.0.4",
    "@envelop/validation-cache": "^6.0.4",
    "@graphql-codegen/cli": "catalog:",
    "@graphql-codegen/typescript": "catalog:",
    "@graphql-tools/merge": "catalog:",
    "@parcel/watcher": "^2.4.0",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.16",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/sanitize-html": "^2.9.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^6.2.1",
    "graphql": "catalog:",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.16.0",
    "jest": "catalog:",
    "nodemon": "^3.0.2",
    "ts-jest": "catalog:",
    "tslib": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "^3.2.4",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "@envelop/core": "^5.3.1",
    "@envelop/graphql-jit": "^8.0.4",
    "@envelop/parser-cache": "^6.0.4",
    "@envelop/validation-cache": "^6.0.4",
    "graphql": "catalog:",
    "zod": "^4.1.11"
  }
}
